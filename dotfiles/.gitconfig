	email = nc6401+gthb@gmail.com
	name = Ruslan Zhenetl
[core]
	eol = lf
[alias]
	amend = commit --amend
	br = branch
	branch-name = rev-parse --abbrev-ref HEAD
	branches = branch -a
	ci = commit
	co = checkout
	pick = cherry-pick
	feature = "!f() { git fresh master; git new $1; }; f"
	fresh = "!f() { local b=${1:-master}; git checkout $b && git pull origin $b; }; f"
	graph = log --graph --all  --decorate --oneline
	hardreset = reset --hard HEAD^
	messages = log --pretty=format:%s
	new = checkout -b
	pop = stash pop
	publish = !git push -u origin $(git branch-name)
	unpublish = push origin --delete
	refresh = !git pull origin $(git branch-name)
	remotes = remote -v
	rmb = branch -D
	st = status
	stashes = stash list
	syncup = pull --rebase origin master
	tags = tag
	uncommit = reset --mixed HEAD^
	unstage = reset HEAD --
	untrack = rm --cached

